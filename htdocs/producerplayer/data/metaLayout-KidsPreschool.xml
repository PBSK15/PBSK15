<?xml version="1.0" encoding="UTF-8"?>
<layout>
	<controls>
		<card id="tpNormalControls">
			<region id="tpBottomRegion">
				<row percentWidth="100" height="50" verticalAlign="middle">
						<group percentWidth="100" height="50" verticalAlign="middle" paddingTop="5">
						<spacer width="5"/>
						<control id="tpRemainingTime"/>
						<control id="tpScrubber"/>
					</group>
					<group paddingLeft="10" height="50" paddingTop="5">
						<control id="pbsCC" height="40" width="40" tooltips=" "/>
					</group>
					<group paddingLeft="10" height="50" paddingTop="5">
						<control id="tpFullScreen" height="40" width="40" tooltips=" "/>
					</group>
					<group paddingLeft="10" height="50" paddingTop="5">
						<control id="tpVolume" height="40" width="46" expandDirection="up"/>
						<spacer width="5"/>
					</group>
				</row>
				<row percentWidth="100" height="0">
					<group percentWidth="100" height="75" verticalAlign="middle" paddingTop="40">
						<spacer width="100%"/>
						<control height="75" width="95" id="tpPrevious" tooltips=""/>
						<spacer width="50%"/>
						<control height="69" width="152" id="tpPlay" tooltips=" , "/>
						<spacer width="50%"/>
						<control height="75" width="95" id="tpNext" tooltips=""/>
						<spacer width="100%"/>
					</group>
				</row>
			</region>
		</card>
		<card id="tpFullScreenControls">
			<region id="tpBottomRegion">
				<row percentWidth="100" height="50" verticalAlign="middle">
					<group height="50" verticalAlign="middle" paddingTop="5">
						<spacer width="5"/>
						<control height="50" width="152" id="tpPlay" tooltips=" , "/>
					</group>
					<group percentWidth="100" height="50" verticalAlign="middle" paddingTop="5">
						<spacer width="5"/>
						<control id="tpRemainingTime"/>
						<control id="tpScrubber"/>
					</group>
					<group paddingLeft="10" height="50" paddingTop="5">
						<control id="pbsCC" height="40" width="40" tooltips=" "/>
					</group>
					<group paddingLeft="10" height="50" paddingTop="5">
						<control id="tpFullScreen" height="40" width="40" tooltips=" "/>
					</group>
					<group paddingLeft="10" height="50" paddingTop="5">
						<control id="tpVolume" height="40" width="46" expandDirection="up"/>
						<spacer width="5"/>
					</group>
				</row>
			</region>
		</card>
	</controls>
</layout>
