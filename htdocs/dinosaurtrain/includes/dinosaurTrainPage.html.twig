        }
       
.mobilify #player #controlrack span {
zoom: 0.6;
}
        
        #player #controlrack span.down{
          top: 10px;
          position:relative;
          padding-bottom: 0px;
        }
 
#player #controlrack button{
width: 100%;
height: 72px;
color: #FF9545;
background: #FFE960;
border-radius: 20px;
font-size: 1.4em;
line-height: 1.5em;
font-family: dino-font;
}

#player #controlrack button .symbol, #player #controlrack button.symbol{
  font-family: arial, sans-serif;
  font-style: normal;
  padding: 0 0 0 0.2em;
  font-size: 2em;
  line-height: 1em;
}

#player #controlrack button .symbol{
  vertical-align: top;
  font-size: 1.5em;
}

.previous .symbol {
-webkit-transform: scale(-1,1);
-moz-transform: scale(-1,1);
-ms-transform: scale(-1,1);
-o-transform: scale(-1,1);
transform: scale(-1,1);
}
 
            #player #controlrack span:hover button{
              background:#FFFA99;
            }
 
               #player #controlrack button img{
                  height:48px;
                }
 
