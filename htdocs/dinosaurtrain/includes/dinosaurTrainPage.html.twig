        #player #controlrack span.play-pause.playing #icon-play
        {
          display:none;
        }
        
        #player #controlrack span.play-pause.playing #icon-pause{
          display:inline;
        }

            #player #controlrack span.play-pause img{
              height:30px;
            }
            
            
