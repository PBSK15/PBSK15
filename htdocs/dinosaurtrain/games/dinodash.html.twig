{% extends 'dinosaurtrain/includes/dinosaurTrainPage.html.twig' %}
{% set title = ' . Dino Dash' %}
{% set desc = 'Meet OrenOllie, two twin Ornithomimus' %}
{% set keywds = 'Oren, Ollie, Ornithomimus, Racing, Running, Science game for kids, Eating, dinosaur facts' %}
{% set onLoadEvent = 'onLoad();' %}


{% block loadContent %}function loadContent(){
if( typeof PBS_FlashCanPlay == 'function' && !PBS_FlashCanPlay(10.1) ){
	document.getElementById("main_content").innerHTML = getNonFlashContent("game");
} else {
	var w	= getSize();
	var obj	= new SWFObject('/dinosaurtrain/media/swfs/shell.swf', 'swfContent', w, '100%', '10.1', '#9f4504')
	obj.addParam('allowscriptaccess', 'always');
	obj.addParam('wmode', 'opaque');
	obj.addParam('FlashVars', 'swf=dinosaurtrain/media/swfs/OrniRace.swf&host='+getHost());
obj.write('main_content');
	fixWidths(w);
}	}{% endblock %}

