{% extends 'dinosaurtrain/includes/dinosaurTrainPage.html.twig' %}
{% set title = ' . Pinecone Pass' %}
{% set desc = 'In this distance game pass the pinecone to the right teammate and help the Claw Stars get down the field!' %}
{% set keywds = 'Dinosaur Train , preschool math, math games, dinosaur games, distance games, sports games, pinecone game' %}
{% set onLoadEvent = 'onLoad();' %}


{% block loadContent %}function loadContent(){
if( typeof PBS_FlashCanPlay == 'function' && !PBS_FlashCanPlay(10.1) ){
	document.getElementById("main_content").innerHTML = getNonFlashContent("game");
} else {
	var w	= getSize();
	var obj	= new SWFObject('/dinosaurtrain/media/swfs/shell_RTL.swf', 'swfContent', w, '100%', '10.1', '#FFFFFF')
	obj.addParam('allowscriptaccess', 'always');
	obj.addParam('wmode', 'opaque');
	obj.addParam('FlashVars', 'swf=dinosaurtrain/media/swfs/Touchdown.swf&host='+getHost());
obj.write('main_content');
	fixWidths(w);
}	}{% endblock %}

