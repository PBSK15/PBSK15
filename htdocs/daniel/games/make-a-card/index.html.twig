{% extends 'daniel/includes/danielPage.html.twig' %}
{% set pageTitle = 'Playing Make a Card' %}
{% set jsInclude = '' %}
{% set bodyOpeningTag = '<body class="headband-hidden" id="player">' %}


{% block mainContent %}<main>
 <div id="content-container">
  <div id="content-window"><!-- embed flash using swfobject -->
   <script type="text/javascript">                            var domain_path = new String("http://"+window.location.hostname+"/daniel/");              var game_path = new String(domain_path + "assets/swf/cardMaker.swf");              var pageID = new String("CreativityGame");              var flashVars = {                baseUrl: domain_path,                page_id: pageID,                gameUrl: game_path,                cursor: 'true',                traceFunction: 'trace'              };              var params = {                allowScriptAccess: 'always',                wmode: 'transparent'              };              var loader_path = new String("/daniel/assets/swf/daniel_loader.swf");              swfobject.embedSWF(loader_path, "content-window", "960px", "500px", "10.0.0","expressInstall.swf", flashVars, params);            </script>
  </div>
  <div class="parent-tip">
   <p class="parent-tip__text"></p>
  </div>
 </div>
</main>{% endblock %}




{% block lowerBand %}{% endblock %}

