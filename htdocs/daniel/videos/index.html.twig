{% set standardRes = false %}


{% block specificRes %}<script type="text/javascript" src='http://pbskids.org/shell/js/lib/jquery/jquery-1.9.1.min.js'></script><link type="text/css" rel="stylesheet" href="/daniel/assets/css/core.css"><script type="text/javascript">    $(function() {      var platform = navigator.platform.toLowerCase();      if (platform == "ipad" || platform == "iphone") {        $('html').addClass("ios");      }    });  </script>{% endblock %}


