  <div id="video-wrapper">
   <video id="video-player" class="disabled"></video>
  </div>
  <div id="controlrack" class="disabled">
   <div class="scrubber-wrapper"><span class="timer-remaining disabled">00:00</span>
    <div class="scrubber disabled">
     <div class="scrubber-track"><span class="scrubber-thumb"></span>
     </div><!-- .scrubber-track -->
    </div><!-- .scrubber -->
   </div><!-- .scrubber-wrapper --> <a href="/daniel/games/" title="Games" class="to-games"> <img class="nav__image" src="/daniel/assets/img/icon-games.png"> <span class="nav__text">Games</span> </a>
   <div class="playback-buttons"><a class="control-button previous">Prev</a> <a class="control-button play-pause" title="Play/Pause"></a> <a class="control-button next">Next</a> <span id="volume-controls">
     <div id="volume-slider-tooltip"><span class="volume-level"></span>
      <div class="volume-slider disabled">
       <div class="scrubber-track"><span class="scrubber-thumb"></span>
       </div><!-- .scrubber-track -->
      </div><!-- .volume-slider --> <a title="Mute/Unmute" class="toggle-mute control-button"></a>
     </div> <a class="control-button toggle-volume-slider"></a> </span> <a title="Captions On/Off" class="control-button toggle-captions">CC</a>
   </div> <a class="control-button playlist-toggle">Browse</a>
  </div><!-- #controlrack -->
 </section><!-- #video-app --> <!--/ @role="Main" --> <!-- pulled from the Arthur video player demo to work with that demo's playlist call -->
 <div id="playlist-wrapper">
  <div class="btn scroll-up">
   <span class="arrow-up"></span>
  </div>
  <ul id="now-playing-list"><!-- Classes that will be place on li.playlistItem: "epsiode" vs. "clip"; "new-video" only if the clip or episode it new -->
   <li class="playlistItem">
    <div class="thumb-container"><img class="covestackcard_kids" src="">
    </div>
    <div class="video-info"><span class="video-title"></span> <span class="video-duration"></span>
    </div></li>
  </ul><!--/ #now-playing-list -->
  <div class="btn scroll-down">
   <span class="arrow-down"></span>
  </div>
  <div id="playlist-filters"><a class="btn current">All</a> <a class="btn" data-filter-type="type" data-filter="episode">Episodes</a> <a class="btn" data-filter-type="type" data-filter="clip">Clips</a> <a class="btn" data-filter-type="keyword" data-filter="music">Music</a> <a class="btn" data-filter-type="keyword" data-filter="imagination">Imagination</a>
  </div><!--/ #playlist-filters -->
 </div><!--/ #playlist-wrapper -->
