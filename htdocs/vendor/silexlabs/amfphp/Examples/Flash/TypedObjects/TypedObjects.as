package  {		import flash.display.MovieClip;	import flash.events.*;	import flash.net.*;	import fl.controls.Button;	import flash.text.TextField;		public class TypedObjects extends MovieClip {		public var resultTxt:TextField;		public var sendUserVo1Btn:Button; 		public var sendUserVo2Btn:Button; 				private var _netConnection:NetConnection;				public function TypedObjects() {			if(loaderInfo.url.indexOf("file://") == 0){				resultTxt.text = "Please access this example through a server, and not through the file system, as the gateway is set as a relative url. Alternatively you can change the url in the class.";				return;			}			sendUserVo1Btn.addEventListener(MouseEvent.CLICK, sendUserVo1BtnClickHandler);			sendUserVo2Btn.addEventListener(MouseEvent.CLICK, sendUserVo2BtnClickHandler);			_netConnection = new NetConnection();			_netConnection.connect("../../Php/");			registerClassAlias('UserVo1', UserVo1);			registerClassAlias('UserVo2', UserVo2);		}				private function sendUserVo1BtnClickHandler(event:MouseEvent):void{			_netConnection.call("VoService/receiveAndReturnUserVo1", new Responder(handleResult, handleError), new UserVo1());		}				private function sendUserVo2BtnClickHandler(event:MouseEvent):void{			_netConnection.call("VoService/receiveAndReturnUserVo2", new Responder(handleResult, handleError), new UserVo2());		}						private function handleResult(result:Object):void{			resultTxt.text = result.status;		}						private function handleError(error:Object):void{			resultTxt.text  = error.toString();		}					}	}