{% extends 'curiousgeorge/includes/templates/flash_game_page.html.twig' %}
{% set ftrAttributes = 'id="ftr-games" class="tallgame"' %}
{% set pageTitle = 'George Brings Spring' %}

{% block gameFrame %}<div id="flash-game">
	<div id="flashcontent">
		<p>&nbsp;</p>
		<h2>This game requires Flash version 9</h2>
		<h2>You can <a href="https://pbskids.org/cgi-registry/kidsbridge.cgi?context=curiousgeorge_flash_player">download the latest version of Flash</a> for FREE.</h2>
		<object classid="clsid:D27CDB6E-AE6D-11cf-96B8-444553540000"
		 codebase="https://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,0,0"
		 width="724" height="338" id="spring_game2" align="">
		 <param name="movie" value="spring_game2.swf?" />
		 <param name="quality" value="best" />
		 <param name="bgcolor" value="#ffffff" />
			 <embed
				src="spring_game2.swf?"
				quality="best"
				bgcolor="#ffffff"
				width="740"
				height="525"
				name="spring_game2"
				type="application/x-shockwave-flash"
				pluginspage="https://www.macromedia.com/go/getflashplayer">
			  </embed>
		</object>

	</div>

	<div id="nametag" style="margin-left:105px;">
		<img src="/curiousgeorge/games/img/nt_george_brings_spring.gif" height="51" width="438" alt="George Brings Spring" title="" />
		<!-- <p>Parents &amp; Teachers! <a href="https://www.pbs.org/parents/curiousgeorge/site/site_features.html#george_brings_spring">Learn more about George Brings Spring</a>.</p> -->
	</div>

	<p>&nbsp;</p>

</div>

<script type="text/javascript">
    var so = new PBS_SWFObject("spring_game2.swf", "spring_game2", "740", "525", "9", "#ffffff");
    so.setAttribute('redirectUrl', '/curiousgeorge/games/noflash.html');
    so.addParam("quality", "best");
    //PBS_enable_cdn(so);
    so.write("flashcontent");
</script>
<!-- end bbinclude -->{% endblock %}