			<!-- insert onselect kudos before letter sequence, depending on station, for letter panel opening and for timeout -->
				
			<!-- singular: click the letter that makes the sound [insert phoneme] -->
			<asset id="spell_prompt_singular" type="audio" url="audio/spelling/vo_spell_onload_sound.mp3" />
			<!-- plural: click the letters that make the sound [insert phoneme] -->
			<asset id="spell_prompt_plural" type="audio" url="audio/spelling/vo_spell_onload_word_0.mp3" />
			<!-- in the word [insert word] -->
			<asset id="spell_prompt_in_the_word" type="audio" url="audio/spelling/vo_spell_onload_word_1.mp3" />
			
