<game id="cakegame" hasSponsorBar="true">
	<title><![CDATA[SUPER WHY! . GAMES . Cake Builder | PBS KIDS]]></title>
	<swf url="sections/games/cakegame/cakegame.swf" />

	<settings
		difficultyIsAdjustable = "true"
		helpIsActive = "true"
		hasPrintable = "false"
		activeStickersPerGame = "5"
		gameDebug = "false"
	/>
	
	<libraries>
		<library id="burbankbigcdttbdembedded" url="libraries/burbankbigcdttbdembedded.swf" type="font" name="BurbankBigCdTTBdEmbedded" font="BurbankBigCdTTBdEmbeddedFont" />
		<library id="futuratdemeduembedded" url="libraries/futuratdemeduembedded.swf" type="font" name="FuturaTDemEduEmbedded" font="FuturaTDemEduEmbeddedFont"/>
		<library id="alphapig_endscreen" url="libraries/alphapig_endscreen.swf" />
		<library id="prizescreen" url="libraries/activityprizescreen.swf" />
	</libraries>
	
	
	<promos>

		<asset id="princess" location="game/superhero" url="../../../promos/endscreen_promo_sh_princess.jpg" />
		<asset id="whyatt" location="game/rescue" url="../../../promos/endscreen_promo_sw_rescue.jpg" />
		<asset id="wonderred" location="game/rhymenroll" url="../../../promos/endscreen_promo_rhymeroll.jpg" />
		
	</promos>
		
	
