			<!-- 'first thing we need is a baking pan.  pick one.' -->
			<asset id="cake_general_prompt" type="audio" url="audio/cakepan/vo_cake_onload.mp3" />
			
			<!-- ooh.  I love that one.  OK, onto the next step.  To fill in cake batter, we need to spell the word [insert word] -->
			<!-- old 
			<asset id="cake_onselect" type="audio" url="audio/cakepan/vo_cake_onselect.mp3" />
			-->
			
			<!--
				On cake pan select:
				new file: vo_cake_onselect_short.mp3 
				'Ooh, I love that one. Ok, onto the next step' (close panel) 
				this is an edit of vo_cake_onselect.mp3 from beginning through 'next step'.
			-->
			<asset id="cake_onselect_short" type="audio" url="audio/cakepan/vo_cake_onselect_short.mp3" />
			
			<!--
				On cake pan letter panel open and on timeout:
				new file: vo_cake_letter_prompt.mp3 
				'To fill in the cake batter, we need to spell the word [insert word]'
				this is an edit of vo_cake_onselect.mp3 from 'to fill in the cake batter' to the end
			-->
			<asset id="cake_letter_prompt" type="audio" url="audio/cakepan/vo_cake_letter_prompt.mp3" />
				
				
			
