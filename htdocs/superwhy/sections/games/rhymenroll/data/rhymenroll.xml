	<swf url="sections/games/rhymenroll/rhymenroll.swf" /> 
	<!-- how quickly the background and the objects on the screen scroll across -->
	<scrollSpeed>-.08</scrollSpeed>
	<!-- speed at which the player moves towards the mouse -->
	<playerSpeed>.1</playerSpeed>
	<!-- the X location of the player on the screen -->
	<playerXPosition>396</playerXPosition>
	<!-- how far appart items are spaced -->
	<itemDistance>400</itemDistance>
	<!-- how often to release a special piece(ramp) - will have a 1 out of [x] chance of appearing -->
	<releaseSpecial>6</releaseSpecial>
	<!-- available vertical screen positions for objects -->
	<objectPositions>
		<position>75</position>
		<position>170</position>
		<position>265</position>
	</objectPositions>
	<!-- number of levels -->
	<numLevels>3</numLevels>
	<!-- wordsets -->
