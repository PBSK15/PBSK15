<game id="storybookcreator" hasSponsorBar="true">
	<title><![CDATA[SUPER WHY! . GAMES . SUPERWHY'S STORYBOOK CREATOR | PBS KIDS]]></title>
	<swf url="sections/games/storybookcreator/storybookcreator.swf" />
	
	<settings
		difficultyIsAdjustable = "true"
		helpIsActive = "true"
		idleTime = "30000"
	/>

	<email 	sendURL="http://pbskids.org/cgi-registry/tell_a_friend.cgi"
			urlToSend="http://pbskids.org/superwhy/index.html#game/storybookcreator" 
			thanksURL="/superwhy/sections/games/storybookcreator/email/success.html" 
			errorURL="/superwhy/sections/games/storybookcreator/email/error.html">
			<content><![CDATA[Checkout the story I created on the SUPER WHY site!]]></content>		
	</email>
	
	<config>
		<storySelector bookDistance="400" bookSpeed="1" bookMovement="easeOutBack" />
	</config>
	
	<books>
		<book location="sections/games/storybookcreator/books/jack.swf" />
		<book location="sections/games/storybookcreator/books/princess.swf" />
		<book location="sections/games/storybookcreator/books/pigs.swf" />
